{
if hspeed&lt;HValue+global.conveyorSpeed {hspeed+=WalkValue+global.conveyorSpeed friction=0; image_speed=0.3; Facing=0;}// Then slowly increase the speed
}
if keyboard_check(global.key_right) &amp;&amp; place_free(x+2,y) &amp;&amp; !position_meeting(x+2,y+6,WoodPlat) &amp;&amp; stun=0 &amp;&amp; OnSlide=0 &amp;&amp; FromSlide=1 // If the key is pressed
{
if hspeed&lt;HValue+global.conveyorSpeed {hspeed+=WalkValue/3; friction=0; image_speed=0.3; Facing=0;}// Then slowly increase the speed
}

//==============LEFT KEYBOARD==============
if keyboard_check(global.key_left) &amp;&amp; place_free(x-2,y) &amp;&amp; !position_meeting(x+2,y+6,WoodPlat) &amp;&amp; stun=0 &amp;&amp; OnSlide=0 &amp;&amp; FromSlide=0
{
if hspeed&gt;-HValue+global.conveyorSpeed {hspeed-=WalkValue+global.conveyorSpeed friction=0; image_speed=0.3; Facing=1;}
}

if keyboard_check(global.key_left) &amp;&amp; place_free(x-2,y) &amp;&amp; !position_meeting(x+2,y+6,WoodPlat) &amp;&amp; stun=0 &amp;&amp; OnSlide=0 &amp;&amp; FromSlide=1
{
if hspeed&gt;-HValue+global.conveyorSpeed {hspeed-=WalkValue/3 friction=0; image_speed=0.3; Facing=1;}
}

//===========LIMIT SPEEDS=========
//////If Polo is in normal mode then reduce speed normally
if FromSlide=0
{
if hspeed&gt;HValue+global.conveyorSpeed &amp;&amp; OnSlide=0 { hspeed-=WalkValue}
if hspeed&lt;-HValue+global.conveyorSpeed &amp;&amp; OnSlide=0 { hspeed+=WalkValue}
}
//=============STOPPING===========
if FromSlide=0
{
if not (keyboard_check(global.key_left) &amp;&amp; keyboard_check(global.key_right) ) &amp;&amp; OnSlide=0
if hspeed&gt;0+global.conveyorSpeed {hspeed-=(StopSpeed);}
if hspeed&lt;0+global.conveyorSpeed {hspeed+=(StopSpeed);}
if hspeed&gt;-WalkValue/2 &amp;&amp; hspeed&lt;WalkValue/2 {hspeed=0; if OnGround=1 and Duck=0 {image_speed=0; image_index=0;} }
}
//========LEFT AND RIGHT PRESSED AT THE SAME TIME===
if keyboard_check(global.key_left) &amp;&amp; keyboard_check(global.key_right) &amp;&amp; stun=0 &amp;&amp; OnSlide=0
{
if hspeed&gt;0 {hspeed-=StopSpeed;}
if hspeed&lt;0 {hspeed+=StopSpeed;}
}

if !keyboard_check(global.key_left) &amp;&amp; !keyboard_check(global.key_right) &amp;&amp; stun=0 &amp;&amp; OnSlide=0
{
if hspeed&gt;0 {hspeed-=StopSpeed;}
if hspeed&lt;0 {hspeed+=StopSpeed;}
if OnGround=1 and Duck=1 {image_speed=0; image_index=0;}
}


