sound_play(snd_honk)
if global.player_birthday=1 and global.UseParticle=1
{
repeat(50)
instance_create(other.x+6,other.y+6,confetti)
}
instance_create(other.x+6,other.y+6,monster_pop)
DoubleDone=0
display_combo()
score+=15
}

