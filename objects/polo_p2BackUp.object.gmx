          <argument>
            <kind>1</kind>
            <string>  // only slaves controls this bat
  if (global.master) exit;


//==============RIGHT=============
if keyboard_check(global.key_right) &amp;&amp; place_free(x+2,y)// If the key is pressed
{
if hspeed&lt;HValue {hspeed+=WalkValue; friction=0; image_speed=0.3; Facing=0;}// Then slowly increase the speed
}

//==============LEFT==============
if keyboard_check(global.key_left) &amp;&amp; place_free(x-2,y)
{
if hspeed&gt;-HValue {hspeed-=WalkValue; friction=0; image_speed=0.3; Facing=1;}
}

//===========LIMIT SPEEDS=========
if hspeed&gt;HValue { hspeed-=WalkValue/2}
if hspeed&lt;-HValue { hspeed+=WalkValue/2}

//=============STOPPING===========
if not (keyboard_check(global.key_left) &amp;&amp; keyboard_check(global.key_right) )
if hspeed&gt;0 {hspeed-=StopSpeed;}
if hspeed&lt;0 {hspeed+=StopSpeed;}
if hspeed&gt;-WalkValue/2 &amp;&amp; hspeed&lt;WalkValue/2 {hspeed=0; if OnGround=1 and Duck=0 {image_speed=0; image_index=0;} }

//========LEFT AND RIGHT PRESSED AT THE SAME TIME===
if keyboard_check(global.key_left) &amp;&amp; keyboard_check(global.key_right)
{
if hspeed&gt;0 {hspeed-=StopSpeed;}
if hspeed&lt;0 {hspeed+=StopSpeed;}
}

if !keyboard_check(global.key_left) &amp;&amp; !keyboard_check(global.key_right)
{
if hspeed&gt;0 {hspeed-=StopSpeed;}
if hspeed&lt;0 {hspeed+=StopSpeed;}
if OnGround=1 and Duck=1 {image_speed=0; image_index=0;}
}

//=====ASSIGN SPRITES====
if Facing=0 {RightSprite()}
if Facing=1 {LeftSprite()}
</string>
