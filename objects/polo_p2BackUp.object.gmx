            <string>  // only slaves controls this bat
  if (global.master) exit;


//====Jumping off the ground===
if keyboard_check(global.key_up) &amp;&amp; (OnGround=1 or gravity=0) and Duck=0
    {
    OnGround=0
    vspeed=-VValue
    gravity=GValue
    DoubleDone=0
    DoubleWait=0
    if Grip=0
    sound_play(snd_jump);
    alarm[0]=1    

    }
//============================

//=========DOUBLE JUMP==========
if keyboard_check_pressed(global.key_up) &amp;&amp; OnGround=0 &amp;&amp; DoubleDone=0 &amp;&amp; DoubleWait=1 &amp;&amp; vspeed&gt;-2 &amp;&amp; global.doublejump=1
{
if global.doublejump=1 {vspeed=-(VValue/1.2);}
DoubleDone=1
DoubleWait=0
image_speed=0.2
if Grip=0
sound_play(snd_dbljump);
}

//==========UMBRELLA==========
if global.umbrella=0
{
if vspeed&gt;5{vspeed=5}
}

if global.umbrella=1 and keyboard_check(global.key_umbrella)
{
if vspeed&gt;0.2{vspeed=0.2}
}

if global.umbrella=1 &amp;&amp; keyboard_check(global.key_umbrella) &amp;&amp; polo.Duck=0 &amp;&amp; polo.Grip=0 //Create protection object
UmbrellaProtection.on=1
else
UmbrellaProtection.on=0


//If there's nothing below then OnGround=0
if place_free(x,y+1) and !place_meeting(x,y+1,OneWay) {OnGround=0; alarm[0]=1;}

//If OnGround = 0 or if you're floating, falling, jumping then activate gravity
if OnGround=0
{
gravity=GValue
}
