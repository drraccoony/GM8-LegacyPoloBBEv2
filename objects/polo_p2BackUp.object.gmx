draw=0

if global.hits&gt;=0
{
damaged=1
timeline_index=DamageFlash
alarm[2]=flashtime
global.earthquake=3
hspeed=0
vspeed=0
}

if global.hits=-1
{
PoloDeath()
}


//Destroy the monster
other.draw=0
global.monster_number-=1

}
