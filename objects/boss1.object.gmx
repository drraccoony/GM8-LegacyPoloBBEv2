spit.image_speed=0.1
}
if rain_mode=1
{
with (Marker2)
   {
   if !place_meeting(x,y,danger_nosound)
    {
    spit=instance_create(x+random(12),y-random(12),danger_nosound)
    spit.vspeed=4
    spit.image_speed=0.1
    }
   }

}

if warpRain=1
{
    spit=instance_create(x,y,danger_nosound)
    spit.gravity_direction=random(360)
    spit.direction=spit.gravity_direction
    spit.gravity=0.1
    spit.speed=-3
    spit.image_speed=0.01
    spit.alarm[0]=60

}

