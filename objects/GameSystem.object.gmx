            <string>if global.gamepad=0
exit

visible=1

if instance_exists(polo){

if joystick_check_button(0,global.pad_jump){
keyboard_key_press(vk_up);
temp=global.pad_jump
}

if not joystick_check_button(0,global.pad_jump) and temp=global.pad_jump{
keyboard_key_release(vk_up);
temp=0}

if global.pad_duck=-1{
if joystick_ypos(0)&gt;=1{ //INGAME CONTROLS
keyboard_key_press(vk_down);}
if joystick_ypos(0)&lt;1 and joystick_ypos(0)&gt;-1{
keyboard_key_release(vk_down);}}
else{if joystick_check_button(0,global.pad_duck) and temp=0{
keyboard_key_press(vk_down);
temp=99}
}

if joystick_xpos(0)&gt;=1{
keyboard_key_press(vk_right);}
if joystick_xpos(0)&lt;=-1{
keyboard_key_press(vk_left);}
if joystick_xpos(0)&lt;1 and joystick_xpos(0)&gt;-1{
keyboard_key_release(vk_right);
keyboard_key_release(vk_left);}

if joystick_check_button(0,10) and temp=0{
keyboard_key_press(vk_escape);
keyboard_key_release(vk_escape);
temp=10}


if not joystick_check_button(0,10) and temp=10
temp=0

if not joystick_check_button(0,global.pad_duck) and temp=99{
keyboard_key_release(vk_down);
temp=0}

}else{
// ======================                 ====================== //
// ====================== MENU NAVIGATION ====================== //
// ======================                 ====================== //

if joystick_ypos(0)&gt;=1{ //MENU &amp; WORLD MAP NAVIGATION
keyboard_key_press(vk_down);}
if joystick_ypos(0)&lt;=-1{
keyboard_key_press(vk_up);}
if joystick_ypos(0)&lt;1 and joystick_ypos(0)&gt;-1{
keyboard_key_release(vk_up);
keyboard_key_release(vk_down);}

if joystick_xpos(0)&gt;=1{
keyboard_key_press(vk_right);}
if joystick_xpos(0)&lt;=-1{
keyboard_key_press(vk_left);}
if joystick_xpos(0)&lt;1 and joystick_xpos(0)&gt;-1{
